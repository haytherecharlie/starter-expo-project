{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "api/*": ["./src/api/*"],
      "config/*": ["./src/config/*"],
      "services/*": ["./src/services/*"],
      "ui/*": ["./src/ui/*"]
    },
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true
  },
  "include": ["src"]
}
